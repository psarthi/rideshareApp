<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.digitusrevolution.rideshare"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LandingPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".UserRegistrationActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".OtpVerificationActivity">
        </activity>
        <activity android:name=".MobileRegistrationActivity">
        </activity>
    </application>

</manifest>